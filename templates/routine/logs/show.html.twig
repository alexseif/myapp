<!DOCTYPE html>

<title>RoutineLog</title>

{% block body %}
    <h1>RoutineLog</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ routine_log.id }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ routine_log.createdAt ? routine_log.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ routine_log.updatedAt ? routine_log.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('routine_log_index') }}">back to list</a>

    <a href="{{ path('routine_log_edit', {'id': routine_log.id}) }}">edit</a>

    {{ include('routine/logs/_delete_form.html.twig') }}
{% endblock %}
